{
  "name": "movie-search",
  "version": "1.0.0",
  "description": "",
  "main": "server/dist/server.js",
  "scripts": {
    "start": "node server/dist/server.js",
    "start:dev": "concurrently \"cd server && npm run watch\" \"wait-on tcp:3001 && cd client && npm run dev\"",
    "server": "cd server && npm start",
    "server:dev": "cd server && npm run dev",
    "install": "cd server && npm i && cd ../client && npm i",
    "client:build": "cd client && npm run build",
    "client:dev": "cd client && npm run dev",
    "seed": "cd server && npm run seed",
    "build": "concurrently \"cd server && npm run build\" \"cd client && npm run build\"",
    "render-build": "npm install --include=dev && npm run build",
    "cypress": "npx cypress open",
    "test": "npx cypress run --browser electron --headless",
    "test-component": "npx cypress run -- component",
    "test-gui": "npx cypress open --detached"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.12.11",
    "@types/react": "^19.0.8",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.2",
    "react": "^19.0.0",
    "react-router-dom": "^7.1.5"
  },
  "devDependencies": {
    "@cypress/react": "^9.0.0",
    "@testing-library/cypress": "^10.0.3",
    "@types/cypress": "^1.1.6",
    "@types/react-router-dom": "^5.3.3",
    "cypress": "^14.0.3",
    "cypress-vite": "^1.6.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "wait-on": "^7.2.0"
  }
}
